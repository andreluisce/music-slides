{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Electron",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/nextron",
      "windows": {
        "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/nextron.cmd"
      },
      "args" : ["."],
      "outputCapture": "std"
    }
  ]
}